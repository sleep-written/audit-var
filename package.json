{
    "name": "audit-var",
    "version": "2.0.0",
    "type": "module",
    "description": "Inspects variables according to a defined structure",
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "exports": {
        ".": {
            "require": "./dist/cjs/index.js",
            "import": "./dist/esm/index.js"
        }
    },
    "scripts": {
        "test": "ava",
        "start": "npx ts-node/esm ./src/index.ts",
        "build": "./package.sh build",
        "watch": "./package.sh watch",
        "clean": "./package.sh clean",
        "prepare": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/sleep-written/audit-var"
    },
    "keywords": [
        "template",
        "auditor",
        "audit",
        "commonjs",
        "module",
        "ts"
    ],
    "author": "sleep-written",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/sleep-written/audit-var/issues"
    },
    "homepage": "https://github.com/sleep-written/audit-var#readme",
    "devDependencies": {
        "@types/node": "^18.0.1",
        "ava": "^4.3.0",
        "ts-node": "^10.8.2",
        "typescript": "^4.7.4"
    }
}
